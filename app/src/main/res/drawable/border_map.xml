<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start="1dp"
        android:end="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:color="#d7d7d7"/>
            <solid android:color="#FFFFFF"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
</layer-list>