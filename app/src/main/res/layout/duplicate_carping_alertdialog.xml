<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:backgroundTint="@color/white"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="중복된 차박지를 등록할 수 없어요!\n다른 곳에 등록해주세요:)"
        android:textSize="13sp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginLeft="48dp"
        android:layout_marginRight="48dp"
        android:layout_marginTop="37dp"/>

    <ImageView
        android:id="@+id/duplicate_img"
        android:layout_width="164.5dp"
        android:layout_height="113.6dp"
        android:layout_gravity="center"
        android:layout_marginTop="20.2dp"/>
    <Button
        android:id="@+id/okbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="확인"
        android:backgroundTint="@color/black"
        android:background="@drawable/purple_border_round"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="15sp"
        android:paddingLeft="71dp"
        android:paddingRight="71dp"
        android:paddingTop="14.68dp"
        android:paddingBottom="14dp"
        android:layout_gravity="center"
        android:layout_marginBottom="30dp"/>
</LinearLayout>